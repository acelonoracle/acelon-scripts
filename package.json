{
  "name": "youves-acurast-jobs",
  "version": "1.0.0",
  "author": "",
  "main": "dist/stxtz/index.js",
  "devDependencies": {
    "@types/node": "^24.0.4",
    "prettier": "^3.0.0",
    "typescript": "^5.0.0"
  },
  "description": "Youves Acurast Jobs - Tezos blockchain integration with Acurast",
  "engines": {
    "bun": ">=1.0.0"
  },
  "keywords": [
    "tezos",
    "acurast",
    "blockchain",
    "stxtz",
    "taquito"
  ],
  "license": "MIT",
  "scripts": {
    "build:stxtz": "NODE_ENV=production bun build scripts/stxtz/index.ts --outdir dist/stxtz --target node --minify --format cjs",
    "build:peaq": "NODE_ENV=production bun build scripts/peaq/index.ts --outdir dist/peaq --target node --minify --format cjs",
    "start:stxtz": "bun run dist/stxtz/index.js",
    "start:peaq": "bun run dist/peaq/index.js",
    "dev:stxtz": "bun --watch scripts/stxtz/index.ts",
    "dev:peaq": "bun --watch scripts/peaq/index.ts",
    "clean": "rm -rf dist",
    "prebuild": "bun run clean",
    "format": "prettier --write scripts/**/*.{ts,tsx,json} *.{json,md}",
    "format:check": "prettier --check scripts/**/*.{ts,tsx,json} *.{json,md}",
    "test": "bun test",
    "test:watch": "bun test --watch"
  },
  "dependencies": {
    "@acelon/acelon-sdk": "^1.4.4",
    "ethers": "^6.14.4"
  },
  "packageManager": "yarn@4.7.0+sha512.5a0afa1d4c1d844b3447ee3319633797bcd6385d9a44be07993ae52ff4facabccafb4af5dcd1c2f9a94ac113e5e9ff56f6130431905884414229e284e37bb7c9"
}
